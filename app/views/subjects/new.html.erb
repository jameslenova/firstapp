
<% provide(:title, 'New Subject') %>
<h1>New Subject</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@subject) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      

      <%= f.submit "Create subject", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>