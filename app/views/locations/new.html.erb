
<% provide(:title, 'New Location') %>
<h1>New Location</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@location) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      

      <%= f.submit "Create location", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>